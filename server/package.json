{
  "name": "tinyapp",
  "version": "0.0.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "nodemon ./src/app.js --exec babel-node",
    "start:w": "nodemon --watch ./src --exec babel-node ./src/app.js",
    "production:start": "node ./dist/app.js",
    "inspect": "babel-node src/app.js --inspect-brk",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "db:create": "node_modules/.bin/create_database",
    "db:drop": "node_modules/.bin/drop_database",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:reset": "npm run db:drop; npm run db:create; npm run db:migrate"
  },
  "engines": {
    "node": "8.9.1"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.1.4",
    "apollo-client": "^1.9.3",
    "apollo-client-preset": "0.1.0-1",
    "apollo-server-express": "^1.1.7",
    "babel-preset-stage-3": "^6.24.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "es6-promise": "^4.1.1",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphql": "^0.11.7",
    "graphql-client": "^2.0.0",
    "graphql-tag": "^2.5.0",
    "graphql-tools": "^2.5.1",
    "lodash": "^4.17.4",
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-watch": "^2.0.7",
    "babel-preset-env": "^1.6.1",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2"
  }
}
